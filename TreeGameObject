--// Scanner Script by ChatGPT x Cracker

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Chuỗi chứa kết quả scan
local result = "=== Game Scanner Result ===\n"
result = result .. "PlaceId: " .. game.PlaceId .. "\n\n"

-- Hàm dựng đường dẫn object
local function getPath(obj)
    local path = obj.Name
    local parent = obj.Parent
    while parent and parent ~= game do
        path = parent.Name .. "." .. path
        parent = parent.Parent
    end
    return path
end

-- Bắt đầu quét
for _, obj in ipairs(game:GetDescendants()) do
    local note = ""

    if obj:IsA("Folder") then
        note = " -- Folder --"
    elseif obj:IsA("Model") then
        note = " -- Model --"
    elseif obj:IsA("Humanoid") then
        note = " -- Character.Humanoid--"
    elseif obj:IsA("Player") then
        note = " -- Player Object--"
    end

    result = result .. getPath(obj) .. " [" .. obj.ClassName .. "]" .. note .. "\n"
end

-- Debug print (executor sẽ in ra console)
print(result)

--// Tạo UI
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")

local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 400, 0, 200)
Frame.Position = UDim2.new(0.5, -200, 0.5, -100)
Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Frame.Parent = ScreenGui

local Label = Instance.new("TextLabel")
Label.Size = UDim2.new(1, 0, 0.5, 0)
Label.Text = "Finished scanning!"
Label.TextColor3 = Color3.fromRGB(0,255,0)
Label.BackgroundTransparency = 1
Label.Parent = Frame

local Button = Instance.new("TextButton")
Button.Size = UDim2.new(0.6, 0, 0.3, 0)
Button.Position = UDim2.new(0.2, 0, 0.6, 0)
Button.Text = "Copy to Clipboard"
Button.Parent = Frame

-- Khi bấm nút -> copy vào clipboard
Button.MouseButton1Click:Connect(function()
    if setclipboard then
        setclipboard(result)
        Label.Text = "Copied!"
        Label.TextColor3 = Color3.fromRGB(0,200,255)
    else
        Label.Text = "Executor không hỗ trợ setclipboard!"
        Label.TextColor3 = Color3.fromRGB(255,0,0)
    end
end)
